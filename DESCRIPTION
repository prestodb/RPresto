Package: RPresto
Title: DBI Connector to Presto
Version: 1.3.0
Authors@R: c(
    person('Onur Ismail', 'Filiz', , 'onur@fb.com', role=c('aut', 'cre')),
    person('Sergey', 'Goder', , 'sgoder@fb.com', role='aut'),
